Match exec "onsubnet 10.10." host ssh.beltran.cc
 HostName network-unraid
 Port 3222

Match exec "onsubnet --not 10.10.20." host ssh.beltran.cc
 HostName ssh.beltran.cc
 Port 22
 ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h  
  
Host unraid.beltran.cc
 HostName network-unraid
 User root

# ---------CODER CONFIG-------------
Host coder.*
    ConnectTimeout=0
    StrictHostKeyChecking=no
    UserKnownHostsFile=/dev/null
    LogLevel ERROR
    ProxyCommand /usr/local/bin/coder --global-config /home/ephraim/.config/coderv2 ssh --stdio --ssh-host-prefix coder. %h

Host *.coder
    ConnectTimeout=0
    StrictHostKeyChecking=no
    UserKnownHostsFile=/dev/null
    LogLevel ERROR

Match host *.coder !exec "/usr/local/bin/coder connect exists %h"
    ProxyCommand /usr/local/bin/coder --global-config /home/ephraim/.config/coderv2 ssh --stdio --hostname-suffix coder %h
    
Host vite.*
    ConnectTimeout=0
    StrictHostKeyChecking=no
    Compression yes
    LocalForward 5173 localhost:5173
    UserKnownHostsFile=/dev/null
    LogLevel ERROR
    ProxyCommand /usr/local/bin/coder --global-config /home/ephraim/.config/coderv2 ssh --stdio --ssh-host-prefix coder. %h

Match host *.vite !exec "/usr/local/bin/coder connect exists %h"
    ConnectTimeout=0
    StrictHostKeyChecking=no
    Compression yes
    LocalForward 5173 localhost:5173
    UserKnownHostsFile=/dev/null
    LogLevel ERROR
    ProxyCommand /usr/local/bin/coder --global-config /home/ephraim/.config/coderv2 ssh --stdio --hostname-suffix coder %h
    
# ------------END-CODER------------
